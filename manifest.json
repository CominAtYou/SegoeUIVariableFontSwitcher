{
    "name": "Segoe UI Variable Font Switcher",
    "description": "Switch the font on websites to Segoe UI Variable Display",
    "version": "1.2.1",
    "manifest_version": 3,
    "content_scripts": [
        {
            "css": [
                "twitter/FontInject.css"
            ],
            "matches": [
                "https://twitter.com/*"
            ]
        },
        {
            "css": [
                "github/FontInject.css"
            ],
            "matches": [
                "https://github.com/*"
            ]
        }
    ]
}
